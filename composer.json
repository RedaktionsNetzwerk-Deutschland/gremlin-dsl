{
    "name": "rnd/gremlin-dsl",
    "description": "Gremlin DSL PHP integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Jansen",
            "email": "dominik.jansen@rnd.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "RND\\GremlinDSL\\": "src/"
        },
        "files": [
            "src/loader.php"
        ]
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "nette/php-generator": "^3.4",
        "symfony/console": "^5.1",
        "symfony/var-dumper": "^5.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "cs": "vendor/bin/phpcs",
        "cs:fix": "vendor/bin/phpcbf"
    },
    "suggest": {
        "brightzone/gremlin-php": "gremlin-server client for php"
    }
}
