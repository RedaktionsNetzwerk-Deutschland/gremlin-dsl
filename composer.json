{
  "name": "specialweb/gremlin-dsl",
  "description": "Gremlin DSL PHP integration",
  "version": "0.5.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dominik Jansen",
      "email": "dominik@specialweb.app"
    }
  ],
  "autoload": {
    "psr-4": {
      "SpecialWeb\\GremlinDSL\\": "src/"
    },
    "files": [
      "resources/loader.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SpecialWeb\\GremlinDSL\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "nette/php-generator": "^v4.0.0",
    "symfony/console": "^6.0",
    "symfony/var-dumper": "^6.0",
    "squizlabs/php_codesniffer": "^3.7",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "cs": "vendor/bin/phpcs",
    "cs:fix": "vendor/bin/phpcbf",
    "test": "vendor/bin/phpunit -c phpunit.xml.dist"
  },
  "suggest": {
    "brightzone/gremlin-php": "gremlin-server client for php"
  },
  "replace": {
    "rnd/gremlin-dsl": "*"
  }
}
